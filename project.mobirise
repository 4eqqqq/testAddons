tle=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\r\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"rgb(7, 9, 21)\" condition=\"overlay && bg.type !== 'color'\">\r\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\r\n    <!-- End block parameters -->\r\n    </mbr-parameters>\r\n\r\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\r\n    </div>\r\n\r\n    <div class=\"container align-center\">\r\n        <div class=\"row justify-content-md-center\">\r\n            <div class=\"mbr-white col-md-8\">\r\n                <div class=\"mbr-media show-modal align-center \" mbr-if=\"showVideoPopUp\" data-modal=\".modalWindow\">\r\n                    <span mbr-icon class=\"mbri-play mbr-iconfont socicon\"></span>\r\n                </div>\r\n                <div mbr-text \r\n                     class=\"icon-description align-center font-italic  mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showDescription && showVideoPopUp\" \r\n                     data-app-selector=\".icon-description\">\r\n                    Watch video\r\n                </div>\r\n                \r\n                <h1 class=\"mbr-section-title align-center mbr-white my-4 mbr-bold mbr-fonts-style\" \r\n                    mbr-theme-style=\"display-1\" \r\n                    mbr-if=\"showTitle\" \r\n                    data-app-selector=\".mbr-section-title\">LEGAL AND BUSINESS SOLUTIONS</h1>\r\n                <p class=\"mbr-text align-center py-2 mbr-fonts-style\" \r\n                   mbr-theme-style=\"display-7\" \r\n                   mbr-if=\"showText\" \r\n                   data-app-selector=\".mbr-text\">\r\n                        Lorem ipsum dolor sit amet, consectetur adipisicing elit. Exercitatio